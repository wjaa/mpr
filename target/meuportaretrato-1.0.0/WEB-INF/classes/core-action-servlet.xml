<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="true">
	
	
	<bean name="/login" class="br.com.wjaa.controller.impl.LoginAction"
		scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/loginUsuario" class="br.com.wjaa.controller.impl.LoginAction"
			scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/manterUsuario" class="br.com.wjaa.controller.impl.UsuarioAction"
			scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/manterCliente" class="br.com.wjaa.controller.impl.ClienteAction"
			scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/manterPerfume" class="br.com.wjaa.controller.impl.PerfumeAction"
			scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/manterFornecedor" class="br.com.wjaa.controller.impl.FornecedorAction"
			scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/manterLote" class="br.com.wjaa.controller.impl.LoteAction"
			scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/manterVendedor" class="br.com.wjaa.controller.impl.VendedorAction"
			scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/manterEstoque" class="br.com.wjaa.controller.impl.EstoqueAction"
			scope="prototype" lazy-init="false">
	</bean>
	
	<bean name="/manterVenda" class="br.com.wjaa.controller.impl.VendaAction"
			scope="prototype" lazy-init="false">
	</bean>
	

</beans>